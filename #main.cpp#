#include <iostream>
	using std::cout;
	using std::endl;

int main()
{
  int two;
  int three;
  int addition_result;
  int division_result;
  double omg_pi = 3.14;
  double pi_multiply = 2.1;
  double multiply_result;

  two = 2;
  three = 3;
  addition_result = two + three;
  division_result = 3 / 2;
  multiply_result = omg_pi * pi_multiply;

  cout << "Result of integer addition: " << addition_result << endl;
  cout << "Result of double multiplication: " << multiply_result << endl;
  cout << "Result of integer division: " << division_result << endl;

  return 0;
}
